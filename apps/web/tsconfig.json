{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@src/*": [
        "./*"
      ],
      "@atoms/*": [
        "components/atoms/*"
      ],
      "@molecules/*": [
        "components/molecules/*"
      ],
      "@organisms/*": [
        "components/organisms/*"
      ],
      "@pages/*": [
        "pages/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
